---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>{{ site.title }} - Features</title>
 <link href="{{ '/features/atom.xml' | relative_url }}" rel="self"/>
 <link href="{{ site.url | append: site.baseurl | append: '/features' }}/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>{{ site.url }}/features</id>

 {% for post in site.tags.features %}
 <entry>
   <title>{{ post.title | xml_escape }}</title>
   <link href="{{ site.url | append: site.baseurl | append: post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>{{ site.url | append: post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
